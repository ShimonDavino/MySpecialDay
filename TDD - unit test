public int checkIfAcountPresentIsValid()
	{
		int lengthPhone = langth(phoneNumber);
		if(lengthPhone != 10)
		{
			print("the length Phone is not in the right length please chang it to string with 10 chars\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
