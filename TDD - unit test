			return 0;
		}
	}
//----------------------------------------------------------------------------
//range 0-5000 of random function
public int checkIfEventNumberIsInRange()
	{
		if(eventNumber < 0 || eventNumber >5000)
		{
			print("the event Number is out of the range please try to do it again or apeal us\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
