//this program make unitast check that any field have exactly value 
//and some field have the right range or right length
//and check also that few field is equal each

//note: some check mathod possibly to do in generic pattern

//the return value -1 mark that error happened
//the return value 0 mark that the action happened right
//----------------------------------------------------------------------------
public int checkIfUserNameIsValid()
	{
		if(userName.IsString == FALSE)
		{
			print("the user name is not invalid please chang it to string\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
//----------------------------------------------------------------------------
public int checkIfPasswordIsValid()
	{
		if(password.IsString == FALSE)
		{
			print("the password is not invalid please chang it to string\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
//-----------------------------------------------------------------------------
public int checkIfPasswordConfirmIsValid()
	{
		if(passwordConfirm.IsString == FALSE)
		{
			print("the password Confirm is not invalid please chang it to string\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
//----------------------------------------------------------------------------
public int checkIfPasswordIsEqualToConfirmPassword()
	{
		if(password != passwordConfirm)
		{
			print("the password is not equal to confirm password please chang it correctly\n");
			return -1;
		}
		else
		{
			return 0;
		}
	}
//------------------------------------------------------------------------------
